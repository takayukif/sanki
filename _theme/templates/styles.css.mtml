/* ------------------------------------------
                  Pagination
------------------------------------------ */
.page-navigation {
    text-align: center;
}

.page-navigation ul{
    display: block;
    padding-bottom: 10px;
}

.page-navigation li {
    display: inline-block;
    border-bottom: none;
}

.page-navigation li a{
    padding: 0 10px;
}

.page-navigation li a[rel="prev"]::before {
    content: "«";
    margin-right: 0.3em;
}

.page-navigation li a[rel="next"]::after {
    content: "»";
    margin-left: 0.3em;
}

