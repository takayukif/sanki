.navbar {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    margin-bottom: 0;
    padding: 0em 0;
    border-radius: 0;
    background: rgba(0,0,0,0.88);
    z-index: 1000;
    transition :all 0.2s ease-in-out 0s;
}

.sticky {
}

.navbar h1 {
    text-align: left;
}

.navbar h1 img {
    transition :all 0.2s ease-in-out 0s;
}

.sticky h1 img {
}

.navbar-header {
    margin: 0;
}

.navbar-collapse {
    padding-left: 0;
    padding-right: 0;
}

.navbar-nav > li {
    padding: 5px 0;
}

.navbar-nav > li > a {
    padding: 10px 12px;
}

.navbar-nav li > a:hover {
    background: #f1f1f1;
}

.navbar-nav li.active > a {
    background: #f1f1f1;
    color: #111;
}


@media (min-width: 768px) {

.navbar{
	background:url(images/header.png) top repeat-x;
}
.navbar .container{
	width:960px;
	margin:0 auto;
	padding:0;
	position:relative;
	height:68px;
}

.navbar-nav {
	position:absolute;
	right:0;
	top:0;
}


.navbar-nav li{
	margin:0 0 0 1px;
	padding:0;
	float:left;
}
.navbar-nav .li1{width:126px;}
.navbar-nav .li2{width:126px;}
.navbar-nav .li3{width:138px;}
.navbar-nav .li4{width:126px;}
.navbar-nav .li5{width:126px;}
.navbar-nav .li6{width:126px;}


.navbar-nav li a{
	padding:0;
	margin:0;
	display:block;
	background-image:url(images/navi.png);
	height:68px;
	text-indent:-9000px;
}

.navbar-nav .li1 a{background-position:0px 0px;}
.navbar-nav .li2 a{background-position:-127px 0px;}
.navbar-nav .li3 a{background-position:-254px 0px;}
.navbar-nav .li4 a{background-position:-393px 0px;}
.navbar-nav .li5 a{background-position:-520px 0px;}
.navbar-nav .li6 a{background-position:-647px 0px;}

.navbar-nav li a:hover{
	background-image:url(images/navi.png);
}
.press-p .navbar-nav .li1 a,.navbar-nav .li1 a:hover{background-position:0px -68px;}
.spot-p .navbar-nav .li2 a,.navbar-nav .li2 a:hover{background-position:-127px -68px;}
.policy-p .navbar-nav .li3 a,.navbar-nav .li3 a:hover{background-position:-254px -68px;}
.company-p .navbar-nav .li4 a,.navbar-nav .li4 a:hover{background-position:-393px -68px;}
.recruit-p .navbar-nav .li5 a,.navbar-nav .li5 a:hover{background-position:-520px -68px;}
.contact-p .navbar-nav .li6 a,.navbar-nav .li6 a:hover{background-position:-647px -68px;}



}




